{
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "startE": "deno task --env-file start",
    "start": "vite",
    "dev": "vite dev",
    "tools:cleanupDb": "deno run --env -A ./tools/cleanupDb.ts",
    "build": "vite build",
    "preview": "deno serve -A _fresh/server.js",
    "start-production": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "unstable": [
    "kv"
  ],
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ],
      "exclude": [
        "no-namespace"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "fresh": "jsr:@fresh/core@^2.1.0",
    "preact": "npm:preact@^10.27.2",
    "@preact/signals": "npm:@preact/signals@^2.3.1",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.3",
    "vite": "npm:vite@^7.1.3",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "s3_lite_client": "jsr:@bradenmacdonald/s3-lite-client",
    "kvdex": "jsr:@olli/kvdex@3.4.0",
    "@zip-js/zip-js": "jsr:@zip-js/zip-js@^2.7.69",
    "@std/streams": "jsr:@std/streams@1.0.12",
    "zod": "npm:zod@4.1.8"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
